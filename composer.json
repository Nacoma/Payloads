{
  "name": "nacoma/payloads",
  "description": "A strong typed payload library for Laravel.",
  "type": "library",
  "license": ["MIT"],
  "authors": [
    {
      "name": "Cody Mann",
      "email": "nathancodymann@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/contracts": "^8.0 || ^9.0",
    "illuminate/http": "^8.0 || ^9.0",
    "illuminate/database": "^8.0 || ^9.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "symfony/var-dumper": "^5.3"
  },
  "autoload": {
    "psr-4": {
      "Nacoma\\Payloads\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Nacoma\\Payloads\\PayloadServiceProvider"
      ]
    }
  }
}
